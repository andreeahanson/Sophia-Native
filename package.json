{
  "main": "node_modules/expo/AppEntry.js",
  "jest": {
    "verbose": true,
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    }
  },
  "scripts": {
